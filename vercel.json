{
  "version": 2,
  "builds": [
    {
      "src": "maker.js",
      "use": "@vercel/node"
    },
    {
      "src": "created.js",
      "use": "@vercel/node"
    },
    {
      "src": "sendMedia.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/created(.*)",
      "dest": "created.js"
    },
    {
      "src": "/api/sendMedia",
      "dest": "sendMedia.js"
    },
    {
      "src": "/(.*)",
      "dest": "maker.js"
    }
  ]
}
